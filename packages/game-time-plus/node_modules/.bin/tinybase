#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/corey/workspace/game-time-plus-next/node_modules/.pnpm/tinybase@5.4.8_@cloudflare+workers-types@4.20250224.0_prettier@3.5.2_react-dom@19.0.0_r_f119fa6fd70ae136b14596e010fc1b02/node_modules/tinybase/cli/node_modules:/home/corey/workspace/game-time-plus-next/node_modules/.pnpm/tinybase@5.4.8_@cloudflare+workers-types@4.20250224.0_prettier@3.5.2_react-dom@19.0.0_r_f119fa6fd70ae136b14596e010fc1b02/node_modules/tinybase/node_modules:/home/corey/workspace/game-time-plus-next/node_modules/.pnpm/tinybase@5.4.8_@cloudflare+workers-types@4.20250224.0_prettier@3.5.2_react-dom@19.0.0_r_f119fa6fd70ae136b14596e010fc1b02/node_modules:/home/corey/workspace/game-time-plus-next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/corey/workspace/game-time-plus-next/node_modules/.pnpm/tinybase@5.4.8_@cloudflare+workers-types@4.20250224.0_prettier@3.5.2_react-dom@19.0.0_r_f119fa6fd70ae136b14596e010fc1b02/node_modules/tinybase/cli/node_modules:/home/corey/workspace/game-time-plus-next/node_modules/.pnpm/tinybase@5.4.8_@cloudflare+workers-types@4.20250224.0_prettier@3.5.2_react-dom@19.0.0_r_f119fa6fd70ae136b14596e010fc1b02/node_modules/tinybase/node_modules:/home/corey/workspace/game-time-plus-next/node_modules/.pnpm/tinybase@5.4.8_@cloudflare+workers-types@4.20250224.0_prettier@3.5.2_react-dom@19.0.0_r_f119fa6fd70ae136b14596e010fc1b02/node_modules:/home/corey/workspace/game-time-plus-next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tinybase/cli/index.js" "$@"
else
  exec node  "$basedir/../tinybase/cli/index.js" "$@"
fi
